# fonts
font.family        : sans-serif
font.serif         : Times
font.sans-serif    : Helvetica
font.cursive       : Zapf Chancery
font.monospace     : Courier
font.size          : 12.0
text.usetex        : true

# axes
axes.titlesize : 16
axes.labelsize : 16


# lines
lines.linewidth : 2
lines.markersize : 10


# ticks
xtick.labelsize      : large
ytick.labelsize      : large


figure.figsize   : 16, 12
figure.dpi       : 160
savefig.dpi         : 300
savefig.jpeg_quality: 100
